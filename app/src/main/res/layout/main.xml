<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/callVoid_text"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="" />

    <Button
        android:id="@+id/callVoid_button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Call callVoid()" />

    <TextView
        android:id="@+id/getNewData_text"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="" />

    <Button
        android:id="@+id/getNewData_button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Call getNewData()" />

    <TextView
        android:id="@+id/getDataString_text"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="" />

    <Button
        android:id="@+id/getDataString_button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Call getDataString()" />

    <Button
        android:id="@+id/getPresetData_button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Call getPresetData()" />

    <Button
        android:id="@+id/setPresetData_button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Call setPresetData()" />

    <Button
        android:id="@+id/getDataList_button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Call getDataList()" />

    <Button
        android:id="@+id/setDataList_button"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Call setDataList()" />
</LinearLayout>
